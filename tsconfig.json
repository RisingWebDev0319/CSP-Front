{
  "compileOnSave": false,
  "compilerOptions": {
    "module": "commonjs",
    "lib": ["es5", "es2015", "dom", "scripthost"],
    "target": "es5",
    "types": [
        "angular",
        "angular-ui-router",
        "lodash",
        "restangular",
        "angular-toastr",
        "jquery"
    ],
      "declaration": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "inlineSources": true
  },
  "exclude": [
    "bower_components",
    "node_modules",
    "typings"
  ]
}
